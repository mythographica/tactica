{
	"name": "@mnemonica/tactica",
	"version": "0.1.0",
	"description": "TypeScript Language Service Plugin for Mnemonica - generates types for nested constructors",
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"files": [
		"lib/",
		"README.md",
		"LICENSE"
	],
	"scripts": {
		"build": "rm -rf ./lib && npx tsc --pretty",
		"watch": "npx tsc --pretty -w",
		"test": "TS_NODE_PROJECT=./tsconfig.test.json mocha test/**/*.test.ts",
		"lint": "npx eslint --fix ./src"
	},
	"keywords": [
		"mnemonica",
		"typescript",
		"language-service",
		"plugin",
		"types",
		"intellisense"
	],
	"author": "went.out@gmail.com",
	"license": "MIT",
	"peerDependencies": {
		"mnemonica": "^0.9.0",
		"typescript": "^5.0.0"
	},
	"devDependencies": {
		"@types/chai": "^4.3.0",
		"@types/mocha": "^10.0.0",
		"@types/node": "^20.0.0",
		"chai": "^4.3.10",
		"mnemonica": "^0.9.0",
		"mocha": "^10.2.0",
		"ts-node": "^10.9.0",
		"typescript": "^5.3.0"
	},
	"dependencies": {
		"@types/chokidar": "^2.1.0",
		"chokidar": "^3.5.0"
	}
}
